{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "path:default",
    "fs:default",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "$HOME/*"
        }
      ]
    },
    "fs:allow-write-text-file",
    "event:default",
    "window:default",
    "window:allow-close",
    "window:allow-minimize",
    "window:allow-maximize",
    "window:allow-start-dragging",
    "window:allow-toggle-maximize",
    "window:allow-show",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:default",
    "os:default",
    "fs:default",
    "clipboard-manager:default",
    "clipboard-manager:allow-write-text"
  ]
}
